@model LancheListViewModel

@{
    ViewData["Title"] = "List";
}

<div class="text-center p-5">
    <h2>@Model.CategoriaAtual</h2>
</div>

<div class="container">
    <div class="row">
        @foreach (var lanche in Model.Lanches)
        {
            @await Html.PartialAsync("_LanchesResumo", lanche)
        }
    </div>
</div>
